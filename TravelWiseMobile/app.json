{
  "expo": {
    "name": "TravelWise",
    "slug": "travelwisemobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Uygulama fotoğraflarınıza erişmek için izin istiyor.",
          "cameraPermission": "Uygulama kameranıza erişmek için izin istiyor."
        }
      ]
    ],
    "sdkVersion": "53.0.0"
  }
}
